{
  "extends": ["airbnb", "prettier"],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },

  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "no-console": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-undef": 1,
    "arrow-body-style": 0,
    "import/first": 0,
    "import/extensions": 0,
    
    "react/prop-types": "off",
    
    "babel/quotes": 0,
    "babel/semi": 0,
    
    "react/jsx-max-props-per-line": 0,
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
    "react/no-did-mount-set-state": "off"

  }
}